{
  "name": "backend-hijack",
  "version": "1.0.2",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node  src/index.js",
    "uplodate": "npm run format && git add . && git commit -am \"new version\"",
    "postversion": "git push && git push --tags",
    "dev": "nodemon  src/index.js",
    "build": "npm install && npm install --only=dev && npm install --only=production",
    "publish": "lerna publish from-package --contents dist --dist-tag next --force-publish",
    "push": "npm version patch && git commit -am \"new version\" && git push",
    "push-minor": "npm version minor && git push",
    "push-major": "npm version major && git push",
    "compublish": "npm run uplodate && npm version patch"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Anton-ewc/backend-hijack.git"
  },
  "dependencies": {
    "ag-grid-community": "^31.0.0",
    "ag-grid-enterprise": "^31.0.0",
    "dayjs": "^1.11.10",
    "dotenv": "^17.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mysql2": "^3.6.5",
    "pug": "^3.0.2"
  }
}
